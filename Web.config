    <!--UrlRewrite -->
    <section name="urlrewritingnet" restartOnExternalChanges="true" requirePermission="false" type="UrlRewritingNet.Configuration.UrlRewriteSection,UrlRewritingNet.UrlRewriter"/>
    <!--end UrlRewrite-->
  </configSections>
  <!-- UrlRewrite -->
  <urlrewritingnet rewriteOnlyVirtualUrls="true" contextItemsPrefix="QueryString" defaultProvider="RegEx" xmlns="http://www.urlrewriting.net/schemas/config/2006/07">
    <rewrites>

      <add name="Rewrite" virtualUrl="^~/(.*)-p-([0-9]+).aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/products.aspx?p=$2" redirectMode="Permanent" ignoreCase="false"/>
      <add name="Rewrite1" virtualUrl="^~/(.*)-pd-([0-9]+).aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/products-detail.aspx?pd=$2" redirectMode="Permanent" ignoreCase="false"/>
      <add name="Rewrite2" virtualUrl="^~/(.*)-nw-([0-9]+).aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/news.aspx?nw=$2" redirectMode="Permanent" ignoreCase="false"/>
      <add name="Rewrite3" virtualUrl="^~/(.*)-nd-([0-9]+).aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/news-detail.aspx?nd=$2" redirectMode="Permanent" ignoreCase="false"/>
      <add name="Rewrite4" virtualUrl="^~/(.*)-ab-([0-9]+).aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/about-us.aspx?ab=$2" redirectMode="Permanent" ignoreCase="false"/>
      <add name="Rewrite5" virtualUrl="^~/(.*)-ct-([0-9]+).aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/constructions.aspx?ct=$2" redirectMode="Permanent" ignoreCase="false"/>
      <add name="Rewrite6" virtualUrl="^~/(.*)-ctd-([0-9]+).aspx" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/constructions-detail.aspx?ctd=$2" redirectMode="Permanent" ignoreCase="false"/>

      <!--<add name="Rewrite1" virtualUrl="^article/([0-9]+)/([_0-9a-z-]+)" rewriteUrlParameter="ExcludeFromClientQueryString" destinationUrl="~/Default2.aspx?id=$1&amp;title=$2" redirectMode="Permanent" ignoreCase="false"/>-->
    </rewrites>
  </urlrewritingnet>
  <!-- End UrlRewrite -->
  <appSettings>
    <add key="Telerik.ScriptManager.TelerikCdn" value="Disabled"/>
    <add key="Telerik.StyleSheetManager.TelerikCdn" value="Disabled"/>
  </appSettings>
  <connectionStrings>
    <remove name="LocalSqlServer" />
